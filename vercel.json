{
    "version": 2,
    "functions": {
        "api": {
          "runtime": "python3.9"
        }
      },
    "routes": [
      {
        "src": "/learning-api-kadmo/(.*)",
        "dest": "/\\1",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      },
      {
        "src": "/(.*)",
        "dest": "/",
        "methods": ["GET", "POST", "PUT", "DELETE"]
      }
    ],
    "builds": [
      {
        "src": "app.py",
        "use": "@now/python" 
      }
    ]
  }
  